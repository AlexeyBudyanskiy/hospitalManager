@model IEnumerable<HospitalManager.WEB.ViewModels.ExampleViewModel>

@{
    ViewBag.Title = "GetAll";
}

<div class="row">
    <div class="col s12 m8 l6 push-m2 push-l3">
        
        @foreach (var example in Model)
        {
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title">@example.Name</span>
                    <p>
                        @example.Description
                    </p>
                </div>
                <div class="card-action">
                    <a href="@Url.RouteUrl(new {controller = "Example", action = "Edit", id = example.Id})">Edit</a>
                    <a href="@Url.RouteUrl(new {controller = "Example", action = "Delete", id = example.Id})">Delete</a>
                </div>
            </div>
        }
        
    </div>
</div>

